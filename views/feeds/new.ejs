<%include ./../partials/header1 %>
  <!-- Start Bottom Header -->
  <div class="header-bg page-area">
    <div class="home-overly"></div>
    <div class="container">
      <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
          <div class="slider-content text-center" style="padding: 100px;">
            <div class="header-bottom">
              <div class="layer2 wow zoomIn" data-wow-duration="1s" data-wow-delay=".4s">
                <div class="row">
                  <div class="" style="width:70%;margin:auto">
                    <h1 class="text-center">Add New Feed</h1>
                    <form action="/feeds" method="POST" enctype="multipart/form-data">
                      <div class="form-group">
                        <input type="file" accept="image/*" name="image" onchange="loadFile(event)"
                          class="form-control">
                        <img id="output" class="form-control" style="height: 20em;"
                          src="data:image/png;base64," />
                      </div>
                      <div class="form-group">
                        <input type="text" name="name" placeholder="Title" class="form-control">
                      </div>
                      <div class="form-group">
                        <input type="text" name="highlights" placeholder="Highlights" class="form-control">
                      </div>
                      <div class="form-group">
                        <textarea type="text" name="description" placeholder="Description" class="form-control"
                          style="height: 5em;"></textarea>
                      </div>
                      <div class="form-group">
                        <input type="text" name="type" placeholder="Q/A or Experience or Job or Event - post"
                          class="form-control">
                      </div>
                      <div class="actions">
                        <button type="submit" class="btn btn-success btn-block">Submit</button>
                      </div>
                    </form>
                    <a style="text-align:center" href="/feeds"><span
                        class="text-center glyphicon glyphicon-chevron-left"></span>Go Back</a>

                  </div>

                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <%include ./../partials/footer %>