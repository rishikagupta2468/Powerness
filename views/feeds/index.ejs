<!-- Original area -->
<div class="about-area area-padding">
  <div class="container-fluid">
    <div style="padding-left: 80px;padding-top: 18px;">
      <form action="/feeds/search" method="POST" class="form-inline">
        <div class="form-group">
          <input type="text" name="type" placeholder="Q&A/Experience/Job/Event " class="form-control">
        </div>
        <div class="form-group">
          <button class="btn btn-md btn-success btn-block">Search post</button>
        </div>
      </form>
    </div>
    <div class="section-headline text-center">
      <h2>All Posts</h2>
    </div>
    <div id="feeds" class="row text-center">
      <% feeds.forEach(function(feed){%>
      <div class="col-md-3 col-sm-6 col-xs-12">
        <%if(type){%>
        <%if(type=="none"){%>
        <div class="thumbnail all">
          <img src="<%= feed.image %>" alt="<%= feed.name%>" style="width: 15em;height: 15em" ; />
          <div class="caption">
            <h4><%= feed.name %></h4>
            <p><%= feed.description.substring(0,100) %></p>
            <p>
              <a class="btn btn-large center-block btn-primary arrow" href="/feeds/<%= feed._id %>">More Info</a>
            </p>
          </div>
        </div>
        <%}%>
          <%if(type==feed.type){%>
        <div class="thumbnail all">
          <img src="<%= feed.image %>" alt="<%= feed.name%>" style="width: 15em;height: 15em" ; />
          <div class="caption">
            <h4><%= feed.name %></h4>
            <p><%= feed.description.substring(0,100) %></p>
            <p>
              <a class="btn btn-large center-block btn-primary arrow" href="/feeds/<%= feed._id %>">More Info</a>
            </p>
          </div>
        </div>
        <%}%>
        <%}%>
      </div>
      <%}); %>
    </div>
  </div>
</div>

<!-- Original area end-->

